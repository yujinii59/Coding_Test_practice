   "source": [
    "n = int(input())\n",
    "seq = list(map(int, input().split()))\n",
    "dp = {}\n",
    "max_val = 0\n",
    "for i in seq:\n",
    "    dp[i] = i\n",
    "    for num in dp:\n",
    "        if num < i:\n",
    "            dp[i] = max(dp[i], dp[num] + i)\n",
    "            \n",
    "    max_val = max(max_val, dp[i])\n",
    "print(max_val)"
   ]
